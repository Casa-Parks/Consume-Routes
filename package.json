{
  "name": "capa-consume-routes",
  "version": "1.0.1",
  "description": "Casa-Parks' Laravel route consumer.",
  "main": "dist/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "compile": "babel -d dist src",
    "watch:compile": "babel --watch -d dist src",
    "prepublish": "npm run compile",
    "test": "npm run compile && mocha tests/**/*.js --compilers js:babel-core/register",
    "watch:test": "npm run compile && mocha --watch tests/**/*.js --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Casa-Parks/Consume-Routes.git"
  },
  "keywords": [
    "consume",
    "routes",
    "laravel"
  ],
  "author": "Connor S. Parks <Connor@ConnorVG.tv>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Casa-Parks/Consume-Routes/issues"
  },
  "homepage": "https://github.com/Casa-Parks/Consume-Routes#readme",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "mocha": "^3.2.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0"
  }
}
